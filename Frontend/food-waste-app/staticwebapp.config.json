{
    "routes": [
      {
        "route": "/*",
        "rewrite": "/index.html"
      }
    ],
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": [
        "/static/*",  
        "/assets/*",  
        "*.js",       
        "*.css",      
        "*.ico",      
        "*.png",      
        "*.jpg",      
        "*.svg"       
      ]
    }
  }
  